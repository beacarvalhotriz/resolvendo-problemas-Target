public class Soma {
    public static void main(String[] args) {
        int INDICE = 13;
        int SOMA = 0;
        int K = 0;

        while (K < INDICE) {    // O loop 'while' continua executando enquanto K for menor que INDICE
            K = K + 1;    // Incrementa K em 1 a cada iteração
            SOMA = SOMA + K;    // Adiciona o valor de K a variável SOMA
        }

        System.out.println(SOMA);
    }
}
